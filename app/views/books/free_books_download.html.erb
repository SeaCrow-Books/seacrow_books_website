  <% @books.each do |book| %>
  <% if book.manuscript_pdf.attached? %>
  <%= link_to 'Download Manuscript PDF', rails_blob_path(book.manuscript_pdf, disposition: "attachment") %>
<% end %>
<% if book.manuscript_epub.attached? %>
  <%= link_to 'Download Manuscript EPUB', rails_blob_path(book.manuscript_epub, disposition: "attachment") %>
<% end %>
<% end %>